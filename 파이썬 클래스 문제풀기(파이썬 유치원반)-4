# 파이썬 클래스 연습문제 6-4 037
class 차 :
    def __init__(self,바퀴,가격,구동계):
        self.바퀴 = 바퀴
        self.가격 = 가격
        self.구동계 = 구동계

    def 정보(self) :
        print('바퀴수',self.바퀴)
        print('가격',self.가격)

class 자전차(차) :
    def __init__(self,바퀴,가격,구동계):
        차.__init__(self,바퀴,가격,구동계)

    def info(self) :
        print('바퀴수',self.바퀴)
        print('가격',self.가격)
        print('구동계',self.구동계)


class 자동차(차) :
    def __init__(self, 바퀴, 가격):
        차.__init__(self,바퀴,가격)
    
